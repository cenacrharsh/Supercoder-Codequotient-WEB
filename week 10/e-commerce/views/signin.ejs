<div>
  <% if(error) { %>
  <h1><%= error %></h1>
  <% } %>

  <form action="/signin" method="POST">
    <input type="text" name="username" placeholder="Enter Username" required />

    <input
      type="password"
      name="password"
      placeholder="Enter Password"
      required
    />

    <input type="submit" value="Sign In" />
  </form>
</div>
